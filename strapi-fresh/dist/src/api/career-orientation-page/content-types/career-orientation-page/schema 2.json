{
    "kind": "collectionType",
    "collectionName": "career_orientation_pages",
    "info": {
        "singularName": "career-orientation-page",
        "pluralName": "career-orientation-pages",
        "displayName": "Career Orientation Page",
        "description": "Complete career orientation page management with multi-language support"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {
        "i18n": {
            "localized": true
        }
    },
    "attributes": {
        "pageTitle": {
            "type": "string",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "slug": {
            "type": "uid",
            "targetField": "pageTitle",
            "required": true
        },
        "metaDescription": {
            "type": "text",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "metaKeywords": {
            "type": "text",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "canonicalUrl": {
            "type": "string"
        },
        "hero": {
            "type": "component",
            "component": "career-orientation.hero",
            "required": true
        },
        "problemsSection": {
            "type": "component",
            "component": "career-orientation.problems-section"
        },
        "solutionsSection": {
            "type": "component",
            "component": "career-orientation.solutions-section"
        },
        "processSection": {
            "type": "component",
            "component": "career-orientation.process-section"
        },
        "careerPathsSection": {
            "type": "component",
            "component": "career-orientation.career-paths-section"
        },
        "expertSection": {
            "type": "component",
            "component": "career-orientation.expert-section"
        },
        "partnersSection": {
            "type": "component",
            "component": "career-orientation.partners-section"
        },
        "assessmentSection": {
            "type": "component",
            "component": "career-orientation.assessment-section"
        },
        "footerSection": {
            "type": "component",
            "component": "career-orientation.footer-section"
        }
    }
}
